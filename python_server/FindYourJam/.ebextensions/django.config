option_settings:
  "aws:elasticbeanstalk:container:python":
    WSGIPath: FindYourJam/wsgi.py

packages:
  yum:
    postgresql93-devel: []

container_commands:
  01_migrate:
    command: "source /opt/python/run/venv/bin/activate && python manage.py migrate --noinput"
    leader_only: true
  02_wsgipass:
    command: 'echo "WSGIPassAuthorization On"  >> ../wsgi.conf'